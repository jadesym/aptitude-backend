{
  "name": "backend",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --max-warnings=0 src/ --ext .ts,.tsx",
    "start": "node dist/main.js",
    "start:dev": "NODE_ENV=development npm run start",
    "test:unit": "jest ./unit --config jestconfig.json --silent",
    "test:e2e": "jest ./e2e --config jestconfig.json --silent"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.14.5"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.4",
    "@types/mongoose": "5.7.31",
    "@types/validator": "13.1.0",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "axios": "0.19.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.1",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@types/dotenv-flow": "3.0.0",
    "@types/graphql": "14.5.0",
    "apollo-server": "2.16.1",
    "cors": "2.8.5",
    "dotenv-flow": "3.2.0",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "mongoose": "5.9.24",
    "validator": "13.1.1"
  }
}
